prefix=%SYSROOT%/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libtiff
Description:  Tag Image File Format (TIFF) library.
Version: 4.7.0
Libs: -L${libdir} -ltiff
Libs.private: -lzstd -llzma -ljbig -ljpeg -lz -lm 
Cflags: -I${includedir}
Requires.private: libzstd liblzma libjpeg zlib 
